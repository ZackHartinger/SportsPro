@{
    ViewData["Title"] = "";
}
@model List<Product>

<div class="container">
    <h1 class="mt-3 mb-3">SportsPro Technical Support</h1>
    <p>Sports management software for the sports enthusiast</p>

    <h1 class="mt-3 mb-3">Admin</h1>
    <div class="list-group">
        <a class="list-group-item list-group-item-action" asp-controller="product" asp-action="Products">Manage Products</a>
        <a class="list-group-item list-group-item-action" asp-controller="technician" asp-action="Technicians">Manage Technicians</a>
        <a class="list-group-item list-group-item-action" asp-controller="customer" asp-action="Customers">Manage Customers</a>
        <a class="list-group-item list-group-item-action" asp-controller="incident" asp-action="Incidents">Manage Incidents</a>
        <a class="list-group-item list-group-item-action" asp-controller="registration" asp-action="Index">Manage Registrations</a>
    </div>

    <h1 class="mt-3 mb-3">Technicians</h1>
    <div class="list-group">
        <a class="list-group-item list-group-item-action" asp-controller="techincident" asp-action="TechIncident">Update Incident</a>
    </div>
    
    
</div>
